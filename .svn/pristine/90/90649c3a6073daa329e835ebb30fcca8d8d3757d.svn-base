package models;

/**
 *
 * @author Beatriz
 */
public class Souvenir {

    /**
     * Souvenir id.
     */
    private int souvenirid;
    /**
     * Tour id.
     */
    private int tourid;
    /**
     * URL.
     */
    private String souvenirURL;
    /**
     * Downloads.
     */
    private int downloads;

    /**
     * Empty constructor.
     */
    public Souvenir() {
    }

    /**
     * Constructor.
     * @param souvenirid1 souvenir id.
     * @param tourid1 tour id.
     * @param souvenirURL1 url.
     * @param downloads1 number.
     */
    public Souvenir(final int souvenirid1, final int tourid1,
            final String souvenirURL1, final int downloads1) {
        this.souvenirid = souvenirid1;
        this.tourid = tourid1;
        this.souvenirURL = souvenirURL1;
        this.downloads = downloads1;
    }

    /**
     * Get souvenir url.
     * @return url.
     */
    public final String getSouvenirURL() {
        return souvenirURL;
    }

    /**
     * Set souvenir url.
     * @param souvenirURL1 url.
     */
    public final void setSouvenirURL(final String souvenirURL1) {
        this.souvenirURL = souvenirURL1;
    }

    /**
     * Get tour id.
     * @return id.
     */
    public final int getTourid() {
        return tourid;
    }

    /**
     * Set tour id.
     * @param tourid1 id.
     */
    public final void setTourid(final int tourid1) {
        this.tourid = tourid1;
    }

    /**
     * Get souvenir id.
     * @return id.
     */
    public final int getSouvenirid() {
        return souvenirid;
    }

    /**
     * Set souvenir id.
     * @param souvenirid1 id.
     */
    public final void setSouvenirid(final int souvenirid1) {
        this.souvenirid = souvenirid1;
    }

    /**
     * Get number of downloads.
     * @return number of downloads.
     */
    public final int getDownloads() {
        return downloads;
    }

    /**
     * Set number of downloads.
     * @param downloads1 number.
     */
    public final void setDownloads(final int downloads1) {
        this.downloads = downloads1;
    }
}
