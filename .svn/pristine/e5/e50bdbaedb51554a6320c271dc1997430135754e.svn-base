package models;

import java.util.ArrayList;
import java.util.List;

/**
 *
 * @author Beatriz
 */
public class User {
    /**
     * User id.
     */
    private int idUser;
    /**
     * Username.
     */
    private String username;
    /**
     * Passoword.
     */
    private String password;
    /**
     * Email.
     */
    private String email;
    /**
     * Created tour collection.
     */
    private List<Tour> createdTourCollection = new ArrayList<Tour>();
    /**
     * Completed tour collection.
     */
    private List<Tour> completedTourCollection = new ArrayList<Tour>();

    /**
     * Empty constructor.
     */
    public User() {
    }

    /**
     * Constructor.
     * @param idUser1 used id.
     * @param username1 username.
     * @param password1 pass.
     * @param email1 email.
     */
    public User(final int idUser1, final String username1,
            final String password1, final String email1) {
        this.idUser = idUser1;
        this.username = username1;
        this.password = password1;
        this.email = email1;
    }

    /**
     * Get email.
     * @return email.
     */
    public final String getEmail() {
        return email;
    }

    /**
     * Set email.
     * @param email1 email.
     */
    public final void setEmail(final String email1) {
        this.email = email1;
    }

    /**
     * Get user id.
     * @return id.
     */
    public final int getIdUser() {
        return idUser;
    }

    /**
     * Set used id.
     * @param idUser1 id.
     */
    public final void setIdUser(final int idUser1) {
        this.idUser = idUser1;
    }

    /**
     * Get password.
     * @return password.
     */
    public final String getPassword() {
        return password;
    }

    /**
     * Set password.
     * @param password1 password.
     */
    public final void setPassword(final String password1) {
        this.password = password1;
    }

    /**
     * Get tour collection.
     * @return tour collection.
     */
    public final List<Tour> getCreatedTourCollection() {
        return createdTourCollection;
    }

    /**
     * Set tour collection.
     * @param tourCollection collection.
     */
    public final void setCreatedTourCollection(
            final List<Tour> tourCollection) {
        this.createdTourCollection = tourCollection;
    }

    /**
     * Get completed tour collection.
     * @return completed tour collection.
     */
    public final List<Tour> getCompletedTourCollection() {
        return completedTourCollection;
    }

    /**
     * Set completed tour collection.
     * @param completedTourCollection1 completed tour collection.
     */
    public final void setCompletedTourCollection(
            final List<Tour> completedTourCollection1) {
        this.completedTourCollection = completedTourCollection1;
    }

    /**
     * Get username.
     * @return username.
     */
    public final String getUsername() {
        return username;
    }

    /**
     * Set username.
     * @param username1 username.
     */
    public final void setUsername(final String username1) {
        this.username = username1;
    }
}
