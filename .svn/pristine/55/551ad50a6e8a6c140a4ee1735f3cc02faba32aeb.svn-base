package com.geoquest.android;

/**
 * Java class for Geouser.
 * @author Fani .
 */
public class GeoUser {

    /**
     * variable for the id of user.
     */
    private int iduser;
    /**
     * variable for username.
     */
    private String username;
    /**
     * variable for password.
     */
    private String password;

    /**
     * Empty constructor.
     */
    public GeoUser() {
    }
    // constructor

    /**
     * Constructor iduser, username, password.
     * @param iduser1 .
     * @param username1 .
     * @param password1 .
     */
    public GeoUser(final int iduser1, final String username1,
            final String password1) {
        this.iduser = iduser1;
        this.username = username1;
        this.password = password1;
    }

    /**
     * Constructor username and password.
     * @param username1 .
     * @param password1 .
     */
    public GeoUser(final String username1, final String password1) {
        this.username = username1;
        this.password = password1;
    }

    /**
     * getting ID user.
     * @return .
     */
    public final int getID() {
        return this.iduser;
    }

    /**
     * setting id user.
     * @param iduser1 .
     */
    public final void setID(final int iduser1) {
        this.iduser = iduser1;
    }

    /**
     * getting username.
     * @return .
     */
    public final String getUsername() {
        return this.username;
    }

    /**
     * setting username.
     * @param username1 .
     */
    public final void setUsername(final String username1) {
        this.username = username1;
    }

    /**
     * getting password.
     * @return .
     */
    public final String getPassword() {
        return this.password;
    }

    /**
     * setting password.
     * @param password1 .
     */
    public final void setPassword(final String password1) {
        this.password = password1;
    }
}
